@font-face
  font-family 'imagine'
  src url('../fonts/imagine.eot?24551343')
  src url('../fonts/imagine.eot?24551343#iefix') format('embedded-opentype'),
       url('../fonts/imagine.woff?24551343') format('woff'),
       url('../fonts/imagine.ttf?24551343') format('truetype'),
       url('../fonts/imagine.svg?24551343#imagine') format('svg')
  font-weight normal
  font-style normal

[class^="icon-"]:before, [class*=" icon-"]:before
  font-family "imagine"
  font-style normal
  font-weight normal
  speak none
  display inline-block
  text-decoration inherit
  width 1em
  // margin-right .2em
  text-align center
  font-variant normal
  text-transform none
  line-height 1em
  // margin-left .2em

.icon-vsco
  &:before
    content '\e805'

.icon-facebook
  &:before
    content '\e800'

.icon-github
  &:before
    content '\e801'

.icon-linkedin
  &:before
    content '\e803'

.icon-stackoverflow
  &:before
    content '\e804'

.icon-twitter
  &:before
    content '\e802'

.icon-spotify
  &:before
    content '\e806'

